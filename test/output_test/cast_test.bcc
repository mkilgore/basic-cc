
int printf(char *, ...);

int main()
{
    int i = 0;
    char c = 0;
    short s = 0;

    i = 256;
    c = i;

    if (c == 0)
        printf("Case 1");

    s = i;

    if (s == 255)
        printf("Case 2");

    s = 257;
    c = s;

    if (c == 1)
        printf("Case 3");

    s = (short)i;
    c = (char)i;
    c = (short)i;
    i = (char)s;

    return 0;
}

